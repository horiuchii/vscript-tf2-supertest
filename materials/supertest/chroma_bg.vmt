"UnLitGeneric"
{
	"$color" "[0 0 0]"
	"$nofog" "1"
	"$nodecal" "1"
	"$surfaceprop" "Default_silent"
	
	"$world_mins" "[0 0 0]"
	
	$zero 0
	$hundred 100
	
	"$red" "0"
	"$green" "1"
	"$blue" "0"
	
	"Proxies"
	{
		"WorldDims"
		{
			"dummy" "0"
		}
        Clamp
        {
            srcVar1                      $zero
            min                         "$world_mins[0]"
            max                         "$world_mins[0]"
            resultVar                    $red
        }
        
        Clamp
        {
            srcVar1                      $zero
            min                         "$world_mins[1]"
            max                         "$world_mins[1]"
            resultVar                    $green
        }
        
        Clamp
        {
            srcVar1                      $zero
            min                         "$world_mins[2]"
            max                         "$world_mins[2]"
            resultVar                    $blue
        }
		"Divide"
		{
			"srcVar1" "$red"
			"srcVar2" "$hundred"
			"resultVar" "$red"
		}
		"Divide"
		{
			"srcVar1" "$green"
			"srcVar2" "$hundred"
			"resultVar" "$green"
		}
		"Divide"
		{
			"srcVar1" "$blue"
			"srcVar2" "$hundred"
			"resultVar" "$blue"
		}
		"Equals"
		{
			"srcVar1" "$red"
			"resultVar" "$color[0]"
		}
		"Equals"
		{
			"srcVar1" "$green"
			"resultVar" "$color[1]"
		}
		"Equals"
		{
			"srcVar1" "$blue"
			"resultVar" "$color[2]"
		}
	}
}